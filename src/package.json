{
  "name": "nativescript-http",
  "version": "1.0.6",
  "description": "Default nativescript http module with authentication fail callback etc.",
  "main": "http.common.js",
  "types": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "6.0.0",
      "ios": "6.0.1"
    }
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "scripts": {
    "tsc": "tsc -skipLibCheck",
    "build": "npm i && npm run tsc",
    "demo.clean": "cd ../demo && ns platform remove android && ns platform remove ios && rm -rf node_modules",
    "demo.ios": "npm run build && cd ../demo && npm i ../src && ns debug ios --no-hmr --clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AchrafBardan/nativescript-http"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Achraf Bardan",
    "email": "achrafbardan@gmail.com"
  },
  "devDependencies": {
    "@nativescript/core": "^8.0.0",
    "@nativescript/types": "^8.0.0",
    "@nativescript/webpack": "beta",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "typescript": "^4.0.0"
  }
}
